<view class="{{((('cy-card')+' '+('cy-card--'+theme))+' '+customClass)}}"><block a:if="{{title||$slots.title||extra||$slots.extra}}"><view class="{{('cy-card__header')}}"><block a:if="{{title||$slots.title}}"><view class="{{(('cy-card__title')+' '+customClassTitle)}}"><slot name="title"></slot>{{''+title+''}}</view></block><block a:if="{{extra||$slots.extra}}"><view class="{{(('cy-card__extra')+' '+customClassExtra)}}"><slot name="extra"></slot>{{''+extra+''}}</view></block></view></block><block a:if="{{$slots.default}}"><view class="{{(('cy-card__content')+' '+customClassContent)}}"><slot></slot></view></block><block a:if="{{$slots.action}}"><view class="{{(('cy-card__action')+' '+customClassAction)}}"><slot name="action"></slot></view></block></view>