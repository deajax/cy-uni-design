<block a:if="{{visible}}"><view data-event-opts="{{[['tap',[['bindClick',['$event']]]]]}}" class="{{((('cy-notice-bar')+' '+('cy-notice-bar--'+theme))+' '+customClass)}}" catchTap="__e"><view class="{{(('cy-notice-bar__prefix-icon')+' '+customClassPrefixIcon)}}"><slot name="prefix-icon"></slot><block a:if="{{prefixIcon}}"><cy-icon vue-id="06ee9c58-1" name="{{prefixIcon||'information-fill'}}" custom-class="cy-notice-bar-class-name-icon" onVueInit="__l"></cy-icon></block></view><view data-event-opts="{{[['longTap',[['changeAnimationState',['$event']]]]]}}" class="{{(('cy-notice-bar__content-wrap')+' '+customClassWrapper)}}" onLongTap="__e"><block a:if="{{$root.g0}}"><view><swiper class="{{(('cy-notice-bar__content--vertical')+' '+customClassContent)}}" interval="{{interval}}" autoplay="{{true}}" circular="{{true}}" vertical="{{true}}"><swiper-item a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><view class="{{(('cy-notice-bar__content--vertical-item')+' '+customClassItem)}}">{{''+item+''}}</view></swiper-item></swiper></view></block><block a:else><view class="{{((((('cy-notice-bar__content')+' '+(carousel?'cy-notice-bar__marquee':''))+' '+(marquee?'':'cy-notice-bar__content-wrapable'))+' '+(animationPlayState?'animation-play-running':'animation-play-paused'))+' '+customClassContent)}}" style="{{$root.s0}}" animation="{{animationData}}"><block a:if="{{content}}">{{content}}</block><slot name="content"></slot><slot></slot></view></block></view><view data-event-opts="{{[['tap',[['bindSuffix',['$event']]]]]}}" class="{{(('cy-notice-bar__suffix-icon')+' '+customClassSuffixIcon)}}" catchTap="__e"><slot name="suffix-icon"></slot><cy-icon vue-id="06ee9c58-2" name="{{suffixIcon}}" custom-class="cy-notice-bar-class-suffix-icon" onVueInit="__l"></cy-icon></view></view></block>