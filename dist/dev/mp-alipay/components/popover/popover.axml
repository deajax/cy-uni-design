<view class="{{((('cy-popover')+' '+('cy-popover--'+placement))+' '+customClass)}}"><view data-event-opts="{{[['tap',[['onVisibleChange',['$event']]]]]}}" onTap="__e"><slot></slot></view><block a:if="{{show}}"><view class="{{((('cy-popover__content')+' '+($slots.content?'cy-popover__content-custom':''))+' '+customClassContent)}}" style="{{(setContentStyle)}}"><block a:if="{{content}}">{{''+content+''}}</block><slot name="content"></slot></view><view class="cy-popover__arrow" style="{{'border-color:'+(background)+';'}}"></view></block><block a:if="{{show}}"><view data-event-opts="{{[['tap',[['handleOverlay',['$event']]]]]}}" class="{{(('cy-popover__overlay')+' '+customClassOverlay)}}" onTap="__e"></view></block></view>