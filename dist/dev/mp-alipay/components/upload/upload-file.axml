<view class="{{(('cy-upload__files')+' '+customClassFiles)}}"><block a:if="{{!readonly}}"><view data-event-opts="{{[['tap',[['choose',['$event']]]]]}}" class="{{(('cy-upload__files-action')+' '+customClassAction)}}" onTap="__e"><slot></slot></view></block><block a:if="{{list.length>0}}"><view class="{{(('cy-upload__files--list')+' '+customClassList)}}"><cy-cell vue-id="{{'2fbb1d47-1-'+index}}" title="{{item.name}}" a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index" onVueInit="__l" vue-slots="{{['left-icon','right-icon','note']}}"><cy-icon vue-id="{{('2fbb1d47-2-'+index)+','+('2fbb1d47-1-'+index)}}" name="close-line" size="{{40}}" color="#ef4444" slot="left-icon" a:if="{{item.status==='error'}}" onVueInit="__l"></cy-icon><view data-event-opts="{{[['tap',[['delFile',[index]]]]]}}" class="cy-upload__files--close-btn" onTap="__e" slot="right-icon" a:if="{{delIcon&&!readonly}}"><cy-divider vue-id="{{('2fbb1d47-3-'+index)+','+('2fbb1d47-1-'+index)}}" layout="vertical" onVueInit="__l"></cy-divider><cy-icon vue-id="{{('2fbb1d47-4-'+index)+','+('2fbb1d47-1-'+index)}}" name="delete-bin-line" size="{{40}}" color="rgba(0, 0, 0, 0.4)" onVueInit="__l"></cy-icon></view><view data-event-opts="{{[['tap',[['uploadFiles',['$0',index],[[['list','',index]]]]]]]}}" class="cy-upload__files--progress" catchTap="__e" slot="note" a:if="{{item.status==='error'}}">重新上传</view></cy-cell></view></block></view>