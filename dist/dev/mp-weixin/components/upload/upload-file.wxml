<view class="{{['cy-upload__files',customClassFiles]}}"><block wx:if="{{!readonly}}"><view data-event-opts="{{[['tap',[['choose',['$event']]]]]}}" class="{{['cy-upload__files-action',customClassAction]}}" bindtap="__e"><slot></slot></view></block><block wx:if="{{$root.g0>0}}"><view class="{{['cy-upload__files--list',customClassList]}}"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><cy-cell vue-id="{{'2fc51d71-1-'+index}}" title="{{item.name}}" bind:__l="__l" vue-slots="{{['left-icon','right-icon','note']}}"><cy-icon vue-id="{{('2fc51d71-2-'+index)+','+('2fc51d71-1-'+index)}}" name="close-line" size="{{40}}" color="#ef4444" slot="left-icon" wx:if="{{item.status==='error'}}" bind:__l="__l"></cy-icon><view data-event-opts="{{[['tap',[['delFile',[index]]]]]}}" class="cy-upload__files--close-btn" bindtap="__e" slot="right-icon" wx:if="{{delIcon&&!readonly}}"><cy-divider vue-id="{{('2fc51d71-3-'+index)+','+('2fc51d71-1-'+index)}}" layout="vertical" bind:__l="__l"></cy-divider><cy-icon vue-id="{{('2fc51d71-4-'+index)+','+('2fc51d71-1-'+index)}}" name="delete-bin-line" size="{{40}}" color="rgba(0, 0, 0, 0.4)" bind:__l="__l"></cy-icon></view><view data-event-opts="{{[['tap',[['uploadFiles',['$0',index],[[['list','',index]]]]]]]}}" class="cy-upload__files--progress" catchtap="__e" slot="note" wx:if="{{item.status==='error'}}">重新上传</view></cy-cell></block></view></block></view>