blur',[['onBlur',['$event']]]],['confirm',[['onConfirm',['$event']]]],['keyboardheightchange',[['onKeyboardHeightChange',['$event']]]],['nicknamereview',[['onNickNameReview',['$event']]]]]}}" value="{{keyword}}" bindinput="__e" bindfocus="__e" bindblur="__e" bindconfirm="__e" bindkeyboardheightchange="__e" bindnicknamereview="__e"/></block><block wx:if="{{keyword!==''&&clearable&&focused}}"><view data-event-opts="{{[['tap',[['onClear',['$event']]]]]}}" class="cy-input__wrap--clearable-icon" bindtap="__e"><cy-icon vue-id="728a676c-3" name="close-circle-fill" bind:__l="__l"></cy-icon></view></block><block wx:if="{{suffix||$slots.suffix}}"><view class="{{['cy-input__wrap--suffix',customClassSuffix]}}"><block wx:if="{{suffix}}"><text>{{suffix}}</text></block><slot name="suffix"></slot></view></block><block wx:if="{{suffixIcon||$slots['suffix-icon']}}"><view data-event-opts="{{[['tap',[['onSuffixClick',['$event']]]]]}}" class="{{['cy-input__wrap--suffix-icon']}}" bindtap="__e"><slot name="suffix-icon"></slot><cy-icon vue-id="728a676c-4" name="{{suffixIcon.name||suffixIcon}}" size="{{suffixIcon.size||''}}" color="{{suffixIcon.color||''}}" custom-class="cy-input__suffix-icon" bind:__l="__l"></cy-icon></view></block></view><block wx:if="{{$root.g0}}"><view class="{{['cy-input__tips',customClassTips]}}">{{''+tips+''}}</view></block><slot name="tips"></slot></view></view>